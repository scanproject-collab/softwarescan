{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/vite",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://softwarescan.vercel.app",
    "NODE_ENV": "production"
  }
}