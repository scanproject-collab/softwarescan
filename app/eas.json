{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "env": {
        "GOOGLE_SERVICES_JSON": "@env:GOOGLE_SERVICES_JSON"
      },
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:clean :app:assembleRelease"
      },
      "node": "18.18.2"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:clean :app:assembleRelease"
      },
      "node": "18.18.2",
      "env": {
        "GOOGLE_SERVICES_JSON": "@env:GOOGLE_SERVICES_JSON"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "18.18.2",
      "android": {
        "buildType": "app-bundle",
        "image": "latest",
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "GOOGLE_SERVICES_JSON": "@env:GOOGLE_SERVICES_JSON"
        }
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "@env:GOOGLE_SERVICES_JSON"
      }
    },
    "productionApk": {
      "autoIncrement": true,
      "node": "18.18.2",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "gradleCommand": ":app:clean :app:assembleRelease",
        "env": {
          "GOOGLE_SERVICES_JSON": "@env:GOOGLE_SERVICES_JSON"
        }
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "@env:GOOGLE_SERVICES_JSON"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "google-service-account.json",
        "track": "production"
      }
    }
  }
}